# OmniHub Backend Environment Variables

# MongoDB Configuration
MONGO_URL="mongodb://localhost:27017"
DB_NAME="omnihub_database"

# CORS (comma-separated origins)
CORS_ORIGINS="*"

# JWT Configuration
JWT_SECRET="your_super_secret_jwt_key_change_in_production"
JWT_ALGORITHM="HS256"
ACCESS_TOKEN_EXPIRE_MINUTES=1440

# Admin Credentials (used for initial setup)
ADMIN_EMAIL="admin@omnihub.com"
ADMIN_PASSWORD="Admin@123"

# Eyecon API Headers (Required for Eyecon Lookup)
# Get these from Eyecon app traffic analysis
EYECON_E_AUTH_V=REPLACE_ME
EYECON_E_AUTH=REPLACE_ME
EYECON_E_AUTH_C=REPLACE_ME
EYECON_E_AUTH_K=REPLACE_ME
